title_size <- 20
## function for getting the correct sig annotation
get_annotation <- function(p_val) {
if (p_val < 0.001) {
return (list('***', 5.5))
} else if (p_val < 0.01) {
return (list('**', 5.5))
} else if (p_val < 0.05) {
return (list('*', 5.5))
} else if (p_val < 0.1) {
return (list('^', 5.5))
} else {
return (list('NS', 3))
}
}
### (1) sue vehicle A driver
